#include <iostream>
using namespace std;

bool isPowerOfTwo(int n)
{
    if(n == 0)
        return true;
    
    while(n != 1)
    {
        if(n % 2 != 0)
            return false;
        
        n = n / 2;
    }
    
    return true;
}
    
int main() {
	
	int n = 32;
	
	printf("%s", isPowerOfTwo(n)? "true": "false");
}
